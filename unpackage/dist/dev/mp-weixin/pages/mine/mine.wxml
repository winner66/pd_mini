<view class="page"><scroll-view class="pd-content" scroll-y="true" enable-back-to-top="true"><view class="container"><view class="userinfo"><block wx:if="{{!hasUserInfo&&canIUse}}"><button open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">获取我的昵称</button></block><block wx:else><block><image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover" data-event-opts="{{[['tap',[['bindViewTap',['$event']]]]]}}" bindtap="__e"></image><text class="userinfo-nickname">{{"你好 : "+userInfo.nickName}}</text></block></block></view></view><view class="pd-split"></view><view class="weui-cells weui-cells_after-title"><view class="weui-cell weui-cell_access" hover-class="weui-cell_active" data-url="{{pdf.intro}}" data-event-opts="{{[['tap',[['openPdf',['$event']]]]]}}" bindtap="__e"><view class="weui-cell__bd">公司介绍</view><view class="weui-cell__ft weui-cell__ft_in-access"></view></view><navigator class="weui-cell weui-cell_access" url="/pages/mine/contact/contact" hover-class="weui-cell_active"><view class="weui-cell__bd">联系我们</view><view class="weui-cell__ft weui-cell__ft_in-access"></view></navigator><view class="weui-cell weui-cell_access" hover-class="weui-cell_active" data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" bindtap="__e"><view class="weui-cell__bd">清除缓存</view><view class="weui-cell__ft weui-cell__ft_in-access"></view></view><view class="weui-cell weui-cell_access" hover-class="weui-cell_active" data-event-opts="{{[['tap',[['testTap',['$event']]]]]}}" bindtap="__e"><view class="weui-cell__bd">测试入口</view><view class="weui-cell__ft weui-cell__ft_in-access"></view></view></view></scroll-view></view>