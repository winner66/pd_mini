<view class="page"><view class="weui-search-bar"><view class="weui-search-bar__form"><navigator url="/pages/search/search"><view class="weui-search-bar__box"><icon class="weui-icon-search_in-box" type="search" size="14"></icon><input class="weui-search-bar__input" type="text" placeholder="{{key}}" disabled="true"/></view></navigator></view></view><scroll-view class="pd-content" scroll-y="true" croll-with-animation="true" enable-back-to-top="true" scroll-top="{{scrollTop}}" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]],['scroll',[['scroll',['$event']]]]]}}" bindscrolltolower="__e" bindscroll="__e"><view class="weui-panel weui-panel_access"><view class="weui-panel__bd"><block wx:for="{{pdInfos}}" wx:for-item="pd" wx:for-index="index" wx:key="index"><block><view class="weui-cell weui-cell_access" data-id="{{pd.id}}" data-title="{{pd.desc}}" hover-class="weui-cell_active" data-event-opts="{{[['tap',[['nav',['$event']]]]]}}" bindtap="__e"><view class="weui-cell__hd"><image class="pd-icon" src="{{context+'/'+pd.pcIcon}}"></image></view><view class="weui-cell__bd"><view class="pd-cell-label">{{pd.desc}}</view></view></view></block></block><view class="weui-loadmore" hidden="{{nomore}}"><view class="weui-loading"></view><view class="weui-loadmore__tips">正在加载...</view></view><view class="weui-loadmore weui-loadmore_line" hidden="{{!nomore}}"><view class="weui-loadmore__tips weui-loadmore__tips_in-line">已无更多数据</view></view></view></view></scroll-view></view>